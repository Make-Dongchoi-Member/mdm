{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "lastest",
    "@nestjs/axios": "lastest",
    "@nestjs/common": "lastest",
    "@nestjs/config": "lastest",
    "@nestjs/core": "lastest",
    "@nestjs/jwt": "lastest",
    "@nestjs/platform-express": "lastest",
    "@nestjs/platform-socket.io": "lastest",
    "@nestjs/serve-static": "lastest",
    "@nestjs/typeorm": "lastest",
    "@nestjs/websockets": "lastest",
    "axios": "lastest",
    "bcrypt": "lastest",
    "cookie-parser": "lastest",
    "datastructures-js": "lastest",
    "handlebars": "lastest",
    "nodemailer": "lastest",
    "pg": "lastest",
    "reflect-metadata": "lastest",
    "rxjs": "lastest",
    "typeorm": "lastest"
  },
  "devDependencies": {
    "@nestjs/cli": "lastest",
    "@nestjs/schematics": "lastest",
    "@nestjs/testing": "lastest",
    "@types/bcrypt": "lastest",
    "@types/cookie-parser": "lastest",
    "@types/express": "lastest",
    "@types/jest": "lastest",
    "@types/node": "lastest",
    "@types/supertest": "lastest",
    "@typescript-eslint/eslint-plugin": "lastest",
    "@typescript-eslint/parser": "lastest",
    "eslint": "lastest",
    "eslint-config-prettier": "lastest",
    "eslint-plugin-prettier": "lastest",
    "jest": "lastest",
    "prettier": "lastest",
    "source-map-support": "lastest",
    "supertest": "lastest",
    "ts-jest": "lastest",
    "ts-loader": "lastest",
    "ts-node": "lastest",
    "tsconfig-paths": "lastest",
    "typescript": "lastest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "overrides": {
    "@nestjs-modules/mailer": {
      "@nestjs/common": "$@nestjs/common",
      "@nestjs/core": "$@nestjs/core"
    }
  }
}
